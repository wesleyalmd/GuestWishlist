<config>
    <acl>
        <resources>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <netgo_gwishlist translate="title" module="netgo_gwishlist">
                                        <title>Gwishlist</title>
                                    </netgo_gwishlist>
                                </children>
                            </config>
                        </children>
                    </system>
                    <netgo_gwishlist translate="title" module="netgo_gwishlist">
                        <title>Guest Wish List</title>
                        <children>
                            <gwishlist translate="title" module="netgo_gwishlist">
                                <title>Gwishlist</title>
                                <sort_order>0</sort_order>
                            </gwishlist>
                        </children>
                    </netgo_gwishlist>
                </children>
            </admin>
        </resources>
    </acl>
</config>
